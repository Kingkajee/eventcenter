<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="hack.getid"  xmlns:tools="http://schemas.android.com/tools"  android:sharedUserId="android.uid.system">

    <!--Source and patched manifests are merged according to standard build rules:
        http://tools.android.com/tech-docs/new-build-system/user-guide/manifest-merger -->

    <uses-permission
        android:name="android.permission.FAKE_PACKAGE_SIGNATURE" android:protectionLevel="signature|privileged" tools:node="replace" />

    <application>
        <meta-data
            android:name="fake-signature"
            android:value="@string/fake_signature"/>

    </application>
</manifest>
